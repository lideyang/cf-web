<!--底部-->
<div data-am-widget="navbar" class="am-navbar am-cf am-navbar-default goods-detail-footer" id="">
	<ul class="am-navbar-nav am-cf am-avg-sm-3">
		<li >
			<a href="#" class="">
				<span class="am-icon-star-o"></span>
			</a>
		</li>
		<li>
			<button id="orderCommitBtn" href="#" class="am-btn am-btn-primary am-btn-sm am-vertical-align-middle submit">投标</button>
		</li>
		<li data-am-navbar-share>
			<a href="https://github.com/allmobilize/amazeui/issues" class="">
				<span class="am-icon-share-square-o"></span>
			</a>
		</li>
	</ul>
</div>
<!--投标modal-->
<div class="am-modal-actions goods-order-detail-modal" id="orderModal">
    <div class="am-modal-actions-group">
        <ul class="am-list">
            <li class="am-modal-actions-header text-muted">
                请选择投标个数
                <button type="button" class="am-close" data-am-modal-close>&times;</button>
            </li>
            <li>
                <p>总价：<span class="text-info">￥<span id="amountMoneyTxt">12</span></span></p>                
                <p class="range am-center" id="rangeBoxer">
                    <input class="amount" type="text" id="comfirmAmount" name="amount" onkeyup="this.value=this.value.replace(/\D/g,'')" onafterpaste="this.value=this.value.replace(/\D/g,'')" value="1" />
                    <a href="javascript:;;" class="am-badge am-badge-primary am-round min">-</a>
                    <a href="javascript:;;" class="am-badge am-badge-primary am-round max">+</a>
                </p>
            </li>
            <li>
                <button id="comfirmBtn" class="am-btn am-btn-block am-btn-primary am-round submit">确定</button>
            </li>
        </ul>
    </div>
</div>
<!--支付密码modal-->
<div class="am-modal-actions goods-order-pwd-modal" id="tradePwdModal">
    <div class="am-modal-actions-group">
        <ul class="am-list">
            <li class="am-modal-actions-header text-muted">
                余额支付
                <i id="backModalBtn" class="am-header-icon am-icon-angle-left"></i>
            </li>
            <li class="am-form">
                <p class="am-margin-bottom-0">支付金额：<span id="amountMoneyTxt2" class="text-info">0</span>元</p>
                <div class="am-form-group am-form-icon">
                    <i class="am-icon-lock text-info"></i>
                    <input id="loginPwd" name="password" type="password" class="am-form-field" placeholder="请输入支付密码" required pattern="^\d{6}$">
                </div>
            </li>
            <li>
                <button id="" class="am-btn am-btn-block am-btn-primary am-round submit">确定</button>
            </li>
        </ul>
    </div>
</div>
<% include footer %>
<script>
	seajs.use(['goods/orderDetail', 'tools','goods/gameDetail'], function (main, tools,game) {
	    main.init();
        game.init();
	    main.progress('0.4');
	    main.orderComfirm();
	    tools.timer('2016-07-20 10:40:13', 'timerTxt');
	    tools.rangeInput('12','1','15');
	})
</script>